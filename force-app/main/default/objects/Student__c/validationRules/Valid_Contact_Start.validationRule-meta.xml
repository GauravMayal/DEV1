<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valid_Contact_Start</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(BEGINS( Text(Contact__c) , &apos;91&apos;))</errorConditionFormula>
    <errorDisplayField>Contact__c</errorDisplayField>
    <errorMessage>Contact should start with 91</errorMessage>
</ValidationRule>
