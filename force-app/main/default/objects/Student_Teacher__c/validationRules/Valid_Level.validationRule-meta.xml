<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valid_Level</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(
TEXT(Student__r.Level__c  ) 
==  
TEXT(Teacher__r.Teaching_Level__c)
)</errorConditionFormula>
    <errorMessage>Teacher’s teaching level doesn’t match Student’s level</errorMessage>
</ValidationRule>
